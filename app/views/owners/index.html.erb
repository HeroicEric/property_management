<h1> The Owners </h1>
<br>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Company</th>
  </tr>
    <% @owners.each do |owner|%>
  <tr>
    <td><%= owner.first_name %></td>
    <td><%= owner.last_name %></td>
    <td><%= owner.email %></td>
    <td><%= owner.company %></td>
    <td><%= owner.id %></td>
</tr>
  <% end %>
</table>
